    <scriptTask id="scripttask10" name="Number EDRISI" scriptFormat="javascript" activiti:autoStoreVariables="false">
      <script>var signFirst = ' ';
var signSecond = ' ';
var signThird = ' ';
var endSign = execution.getVariable('nSerialNumberRegistrationISI')

if (execution.getVariable('asSignISI') == 'kif'){
	signFirst = '1'
}
if (execution.getVariable('asSignISI') == 'pif'){
	signFirst = '2'
}
if (execution.getVariable('asISIForm') == 'open'){
	signSecond = '1'
}
if (execution.getVariable('asISIForm') == 'interval'){
	signSecond = '2'
}
if (execution.getVariable('asISIForm') == 'closed'){
	signSecond = '3'
}
if (execution.getVariable('asISIType') == 'diversified'){
	signThird = '1'
}
if (execution.getVariable('asISIType') == 'nonDiversified'){
	signThird = '2'
}
if (execution.getVariable('asISIType') == 'venture'){
	signThird = '3'
}
if (execution.getVariable('asISIType') == 'specialized'){
	signThird = '4'
}
if (execution.getVariable('asISIType') == 'qualifying'){
	signThird = '5'
}
if (execution.getVariable('asISIType') == 'exchange'){
	signThird = '6'
}
var edrisiNumber = signFirst + signSecond + signThird + endSign
execution.setVariable('nEDRISI', edrisiNumber)</script>
    </scriptTask>
